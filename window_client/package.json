  "version": "1.0.1",
  "description": "OfferCar AI Windows Client",
  "main": ".webpack/main",
  "scripts": {
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "start": "chcp 65001 >nul && electron-forge start",
    "package": "electron-forge package",
    "package:prod": "cross-env NODE_ENV=production electron-forge package",
